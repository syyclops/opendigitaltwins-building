{
  "@id": "dtmi:com:syyclops:SpaceOccupancy;1",
  "@type": "Interface",
  "displayName": {
    "en": "Space Occupancy"
  },
  "extends": [
    "dtmi:com:syyclops:SpaceComponent;1"
  ],
  "contents": [
    {
      "@type": "Property",
      "name": "isOccupied",
      "displayName": {
        "en": "Is Occupied"
      },
      "writable": true,
      "schema": "boolean",
      "@id": "dtmi:com:syyclops:SpaceOccupancy:isOccupied;1",
      "description": {
        "en": "The isOccupied property indicates whether the space is currently occupied."
      }
    },
    {
      "@type": "Property",
      "name": "peopleCount",
      "displayName": {
        "en": "People Count"
      },
      "writable": true,
      "schema": "double",
      "@id": "dtmi:com:syyclops:SpaceOccupancy:peopleCount;1",
      "description": {
        "en": "The peopleCount property captures the total number of individuals currently in the space."
      }
    },
    {
      "@type": "Property",
      "name": "hasInferredOccupancy",
      "displayName": {
        "en": "Inferred Occupancy"
      },
      "writable": true,
      "schema": "boolean",
      "@id": "dtmi:com:syyclops:SpaceOccupancy:hasInferredOccupancy;1",
      "description": {
        "en": "The hasInferredOccupancy property reflects whether occupancy is inferred from various sensors."
      }
    },
    {
      "@type": [
        "Property",
        "TimeSpan"
      ],
      "name": "dwellTimeAverage",
      "displayName": {
        "en": "Dwell Time Average"
      },
      "writable": true,
      "schema": "double",
      "unit": "second",
      "comment": "Average time spent in the space by all individuals in the space",
      "@id": "dtmi:com:syyclops:SpaceOccupancy:dwellTimeAverage;1",
      "description": {
        "en": "The dwellTimeAverage property calculates the average time occupants spend in the space."
      }
    },
    {
      "@type": [
        "Property",
        "TimeSpan"
      ],
      "name": "entranceDwellTime",
      "displayName": {
        "en": "Entrance Dwell Time"
      },
      "writable": true,
      "schema": "double",
      "unit": "second",
      "comment": "Wait time to enter the space",
      "@id": "dtmi:com:syyclops:SpaceOccupancy:entranceDwellTime;1",
      "description": {
        "en": "The entranceDwellTime property measures the duration individuals spend entering the space."
      }
    },
    {
      "@type": [
        "Property",
        "TimeSpan"
      ],
      "name": "exitDwellTime",
      "displayName": {
        "en": "Exit Dwell Time"
      },
      "writable": true,
      "schema": "double",
      "unit": "second",
      "comment": "Wait time to exit the space",
      "@id": "dtmi:com:syyclops:SpaceOccupancy:exitDwellTime;1",
      "description": {
        "en": "The exitDwellTime property assesses how long individuals take to exit the space."
      }
    },
    {
      "@type": "Property",
      "name": "entranceRate",
      "displayName": {
        "en": "Entrance Rate"
      },
      "writable": true,
      "schema": "double",
      "comment": "e.g. people per second",
      "@id": "dtmi:com:syyclops:SpaceOccupancy:entranceRate;1",
      "description": {
        "en": "The entranceRate property tracks the frequency of individuals entering the space over time."
      }
    },
    {
      "@type": "Property",
      "name": "exitRate",
      "displayName": {
        "en": "Exit Rate"
      },
      "writable": true,
      "schema": "double",
      "comment": "e.g. people per second",
      "@id": "dtmi:com:syyclops:SpaceOccupancy:exitRate;1",
      "description": {
        "en": "The exitRate property measures the frequency of individuals leaving the space over time."
      }
    }
  ],
  "@context": [
    "dtmi:dtdl:context;3",
    "dtmi:dtdl:extension:quantitativeTypes;1"
  ],
  "description": {
    "en": "The SpaceOccupancy interface covers properties related to the occupancy status of a space."
  }
}