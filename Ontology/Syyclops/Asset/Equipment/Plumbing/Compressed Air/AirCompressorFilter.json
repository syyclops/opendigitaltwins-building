{
  "@id": "dtmi:com:syyclops:AirCompressorFilter;1",
  "@type": "Interface",
  "displayName": {
    "en": "Air Compressor Filter"
  },
  "description": {
    "en": "A filter that removes contaminants from the compressed air line to delivery clean air to the equipment."
  },
  "extends": [
    "dtmi:com:syyclops:CompressedAirEquipment;1"
  ],
  "contents": [
    {
      "@type": [
        "Property",
        "VolumeFlowRate"
      ],
      "name": "flowRate",
      "displayName": {
        "en": "Flow Rate"
      },
      "writable": true,
      "schema": "integer",
      "unit": "litrePerSecond",
      "@id": "dtmi:com:syyclops:AirCompressorFilter:flowRate;1",
      "description": {
        "en": "The flowRate property quantifies the volume of air that can pass through the air compressor filter, typically in cubic feet per minute."
      }
    },
    {
      "@type": [
        "Property",
        "ValueAnnotation",
        "Override"
      ],
      "displayName": {
        "en": "Flow Rate Unit"
      },
      "name": "flowRateUnit",
      "annotates": "flowRate",
      "overrides": "unit",
      "schema": "VolumeFlowRateUnit",
      "writable": true,
      "@id": "dtmi:com:syyclops:AirCompressorFilter:flowRateUnit;1",
      "description": {
        "en": "The flowRateUnit property specifies the measurement units used for the flow rate of the air compressor filter."
      }
    },
    {
      "@type": [
        "Property",
        "Pressure"
      ],
      "displayName": {
        "en": "Working Pressure"
      },
      "name": "workingPressure",
      "schema": "double",
      "unit": "bar",
      "writable": true,
      "@id": "dtmi:com:syyclops:AirCompressorFilter:workingPressure;1",
      "description": {
        "en": "The workingPressure property indicates the pressure at which the air compressor filter performs optimally."
      }
    },
    {
      "@type": [
        "Property",
        "ValueAnnotation",
        "Override"
      ],
      "displayName": {
        "en": "Working Pressure Unit"
      },
      "name": "workingPressureUnit",
      "annotates": "workingPressure",
      "overrides": "unit",
      "schema": "PressureUnit",
      "writable": true,
      "@id": "dtmi:com:syyclops:AirCompressorFilter:workingPressureUnit;1",
      "description": {
        "en": "The workingPressureUnit property denotes the unit of measurement for the working pressure of the air compressor filter."
      }
    }
  ],
  "@context": [
    "dtmi:dtdl:context;3",
    "dtmi:dtdl:extension:quantitativeTypes;1",
    "dtmi:dtdl:extension:annotation;1",
    "dtmi:dtdl:extension:overriding;1"
  ]
}