{
  "@id": "dtmi:com:syyclops:UPS;1",
  "@type": "Interface",
  "displayName": {
    "en": "UPS"
  },
  "extends": [
    "dtmi:com:syyclops:ElectricalGenerationStorageEquipment;1"
  ],
  "contents": [
    {
      "@type": [
        "TimeSpan",
        "Property"
      ],
      "displayName": {
        "en": "Runtime"
      },
      "name": "runtime",
      "schema": "double",
      "unit": "minute",
      "writable": true,
      "@id": "dtmi:com:syyclops:UPS:runtime;1",
      "description": {
        "en": "The runtime property indicates the duration that the UPS can provide power during an outage."
      }
    },
    {
      "@type": [
        "Property",
        "ApparentPower"
      ],
      "displayName": {
        "en": "Power Output (KVA)"
      },
      "name": "powerOutputKVA",
      "schema": "double",
      "unit": "kilovoltAmpere",
      "writable": true,
      "@id": "dtmi:com:syyclops:UPS:powerOutputKVA;1",
      "description": {
        "en": "The powerOutputKVA property specifies the kilovolt-amps output capacity of the UPS."
      }
    },
    {
      "@type": [
        "Power",
        "Property"
      ],
      "displayName": {
        "en": "Power Output"
      },
      "name": "powerOutput",
      "schema": "double",
      "unit": "kilowatt",
      "writable": true,
      "@id": "dtmi:com:syyclops:UPS:powerOutput;1",
      "description": {
        "en": "Represents the power output level of the UPS, indicating the amount of electrical power supplied to connected loads."
      }
    },
    {
      "@type": [
        "Voltage",
        "Property"
      ],
      "displayName": {
        "en": "Output Voltage"
      },
      "name": "outputVoltage",
      "schema": "double",
      "unit": "volt",
      "writable": true,
      "@id": "dtmi:com:syyclops:UPS:outputVoltage;1",
      "description": {
        "en": "Defines the voltage level supplied by the UPS to the output load, critical for equipment compatibility."
      }
    },
    {
      "@type": "Property",
      "displayName": {
        "en": "Output Phases"
      },
      "name": "outputPhases",
      "schema": {
        "@type": "Enum",
        "enumValues": [
          {
            "enumValue": "Three",
            "name": "Three",
            "@id": "dtmi:com:syyclops:UPS:Three;1"
          },
          {
            "enumValue": "One",
            "name": "One",
            "@id": "dtmi:com:syyclops:UPS:One;1"
          }
        ],
        "valueSchema": "string",
        "@id": "dtmi:com:syyclops:UPS:ES1;1"
      },
      "writable": true,
      "@id": "dtmi:com:syyclops:UPS:outputPhases;1",
      "description": {
        "en": "Indicates the number of phases in the UPS output, which affects power distribution and equipment operation."
      }
    },
    {
      "@type": [
        "Voltage",
        "Property"
      ],
      "displayName": {
        "en": "Input Voltage"
      },
      "name": "inputVoltage",
      "schema": "double",
      "unit": "volt",
      "writable": true,
      "@id": "dtmi:com:syyclops:UPS:inputVoltage;1",
      "description": {
        "en": "Specifies the voltage level required for the UPS input, necessary for proper operation and efficiency."
      }
    },
    {
      "@type": "Property",
      "displayName": {
        "en": "Input Phases"
      },
      "name": "inputPhases",
      "schema": {
        "@type": "Enum",
        "enumValues": [
          {
            "enumValue": "Three",
            "name": "Three",
            "@id": "dtmi:com:syyclops:UPS:EE3;1"
          },
          {
            "enumValue": "One",
            "name": "One",
            "@id": "dtmi:com:syyclops:UPS:EE4;1"
          }
        ],
        "valueSchema": "string",
        "@id": "dtmi:com:syyclops:UPS:ES2;1"
      },
      "writable": true,
      "@id": "dtmi:com:syyclops:UPS:inputPhases;1",
      "description": {
        "en": "Identifies the number of phases in the UPS input power supply, influencing the UPS's overall performance."
      }
    }
  ],
  "@context": [
    "dtmi:dtdl:context;3",
    "dtmi:dtdl:extension:quantitativeTypes;1"
  ],
  "description": {
    "en": "The UPS interface represents the Uninterruptible Power Supply system within the electrical generation storage."
  }
}