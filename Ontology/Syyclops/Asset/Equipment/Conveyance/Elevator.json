{
  "@id": "dtmi:com:syyclops:Elevator;1",
  "@type": "Interface",
  "displayName": {
    "en": "Elevator"
  },
  "extends": [
    "dtmi:com:syyclops:ConveyanceEquipment;1"
  ],
  "contents": [
    {
      "@type": "Property",
      "description": {
        "en": "The number of people who can fit in a Space."
      },
      "displayName": {
        "en": "Person Capacity"
      },
      "name": "personCapacity",
      "schema": "integer",
      "writable": true,
      "@id": "dtmi:com:syyclops:Elevator:personCapacity;1"
    },
    {
      "@type": [
        "Mass",
        "Property"
      ],
      "displayName": {
        "en": "Weight Capacity"
      },
      "name": "weightCapacity",
      "schema": "double",
      "unit": "kilogram",
      "writable": true,
      "@id": "dtmi:com:syyclops:Elevator:weightCapacity;1",
      "description": {
        "en": "The property indicating the maximum weight capacity the elevator can safely handle."
      }
    },
    {
      "@type": [
        "Property",
        "ValueAnnotation",
        "Override"
      ],
      "displayName": {
        "en": "Weight Capacity Unit"
      },
      "name": "weightCapacityUnit",
      "annotates": "weightCapacity",
      "overrides": "unit",
      "schema": "MassUnit",
      "writable": true,
      "@id": "dtmi:com:syyclops:Elevator:weightCapacityUnit;1",
      "description": {
        "en": "The unit of measurement for the elevator's weight capacity, typically expressed in kilograms or pounds."
      }
    },
    {
      "@type": [
        "Property",
        "Velocity"
      ],
      "displayName": {
        "en": "Maximum Travel Speed"
      },
      "name": "maxTravelSpeed",
      "schema": "double",
      "unit": "metrePerSecond",
      "writable": true,
      "@id": "dtmi:com:syyclops:Elevator:maxTravelSpeed;1",
      "description": {
        "en": "The property defining the maximum speed at which the elevator can travel vertically."
      }
    },
    {
      "@type": [
        "Property",
        "ValueAnnotation",
        "Override"
      ],
      "displayName": {
        "en": "Maximum Travel Speed Unit"
      },
      "name": "maxTravelSpeedUnit",
      "annotates": "maxTravelSpeed",
      "overrides": "unit",
      "schema": "VelocityUnit",
      "writable": true,
      "@id": "dtmi:com:syyclops:Elevator:maxTravelSpeedUnit;1",
      "description": {
        "en": "The unit of measurement for the elevator's maximum travel speed, generally in meters per second or feet per minute."
      }
    },
    {
      "@type": [
        "Property",
        "Length"
      ],
      "displayName": {
        "en": "Maximum Travel Distance"
      },
      "name": "maxTravelDistance",
      "schema": "double",
      "unit": "metre",
      "writable": true,
      "@id": "dtmi:com:syyclops:Elevator:maxTravelDistance;1",
      "description": {
        "en": "The maximum vertical distance the elevator can cover between its lowest and highest stops."
      }
    },
    {
      "@type": [
        "Property",
        "ValueAnnotation",
        "Override"
      ],
      "displayName": {
        "en": "Maximum Travel Distance Unit"
      },
      "name": "maxTravelDistanceUnit",
      "annotates": "maxTravelDistance",
      "overrides": "unit",
      "schema": "LengthUnit",
      "writable": true,
      "@id": "dtmi:com:syyclops:Elevator:maxTravelDistanceUnit;1",
      "description": {
        "en": "The unit of measurement for the maximum travel distance, often referenced in meters or feet."
      }
    },
    {
      "@type": "Property",
      "displayName": {
        "en": "Maximum Landings"
      },
      "name": "maxLandings",
      "schema": "integer",
      "writable": true,
      "@id": "dtmi:com:syyclops:Elevator:maxLandings;1",
      "description": {
        "en": "The maximum number of landings or stops the elevator can serve."
      }
    },
    {
      "@type": "Property",
      "name": "fireServiceAccessElevator",
      "displayName": {
        "en": "Fire Service Access Elevator"
      },
      "writable": true,
      "schema": "boolean",
      "@id": "dtmi:com:syyclops:Elevator:fireServiceAccessElevator;1",
      "description": {
        "en": "Indicates whether the elevator is designated for fire service access."
      }
    },
    {
      "@type": "Property",
      "name": "emergencyCommunication",
      "displayName": {
        "en": "Emergency Communication"
      },
      "writable": true,
      "schema": {
        "@type": "Object",
        "fields": [
          {
            "name": "twoWayVoice",
            "displayName": {
              "en": "Two-Way Voice"
            },
            "schema": "boolean",
            "@id": "dtmi:com:syyclops:Elevator:twoWayVoice;1"
          },
          {
            "name": "twoWayMessageDisplay",
            "displayName": {
              "en": "Two-Way Message Display"
            },
            "schema": "boolean",
            "@id": "dtmi:com:syyclops:Elevator:twoWayMessageDisplay;1"
          },
          {
            "name": "oneWayVideo",
            "displayName": {
              "en": "One-Way Video"
            },
            "schema": "boolean",
            "@id": "dtmi:com:syyclops:Elevator:oneWayVideo;1"
          }
        ],
        "@id": "dtmi:com:syyclops:Elevator:OS1;1"
      },
      "@id": "dtmi:com:syyclops:Elevator:emergencyCommunication;1",
      "description": {
        "en": "Specifies the presence of emergency communication systems within the elevator."
      }
    }
  ],
  "@context": [
    "dtmi:dtdl:context;3",
    "dtmi:dtdl:extension:quantitativeTypes;1",
    "dtmi:dtdl:extension:annotation;1",
    "dtmi:dtdl:extension:overriding;1"
  ],
  "description": {
    "en": "An interface representing an elevator asset that facilitates vertical transport of passengers or goods."
  }
}