{
  "@id": "dtmi:com:syyclops:RefrigerationEvaporatorUnit;1",
  "@type": "Interface",
  "displayName": {
    "en": "Refrigeration Evaporator Unit"
  },
  "description": {
    "en": "An stand-alone evaporator consisting of a fan and coil typically used to cool walk-in coolers or freezers."
  },
  "extends": [
    "dtmi:com:syyclops:RefrigerationEquipment;1"
  ],
  "contents": [
    {
      "@type": "Property",
      "displayName": {
        "en": "Refrigerant Type"
      },
      "name": "refrigerantType",
      "schema": "string",
      "writable": true,
      "@id": "dtmi:com:syyclops:RefrigerationEvaporatorUnit:refrigerantType;1",
      "description": {
        "en": "The refrigerant type property identifies the type of refrigerant used in the refrigeration evaporator unit."
      }
    },
    {
      "@type": "Component",
      "displayName": {
        "en": "fan"
      },
      "name": "fan",
      "schema": "dtmi:com:syyclops:Fan;1",
      "@id": "dtmi:com:syyclops:RefrigerationEvaporatorUnit:fan;1",
      "description": {
        "en": "The fan component regulates airflow through the refrigeration evaporator unit for enhanced heat exchange."
      }
    },
    {
      "@type": [
        "Property",
        "EnergyRate"
      ],
      "displayName": {
        "en": "Nominal Cooling Capacity"
      },
      "name": "nominalCoolingCapacity",
      "schema": "double",
      "unit": "britishThermalUnitPerHour",
      "writable": true,
      "@id": "dtmi:com:syyclops:RefrigerationEvaporatorUnit:nominalCoolingCapacity;1",
      "description": {
        "en": "The nominal cooling capacity property specifies the cooling performance capability of the evaporator at standard operations."
      }
    },
    {
      "@type": [
        "Property",
        "ValueAnnotation",
        "Override"
      ],
      "displayName": {
        "en": "Nominal Cooling Capacity Unit"
      },
      "name": "nominalCoolingCapacityUnit",
      "annotates": "nominalCoolingCapacity",
      "overrides": "unit",
      "schema": "PowerUnit",
      "writable": true,
      "@id": "dtmi:com:syyclops:RefrigerationEvaporatorUnit:nominalCoolingCapacityUnit;1",
      "description": {
        "en": "The nominal cooling capacity unit indicates the measurement unit for the cooling capacity of the evaporator."
      }
    },
    {
      "@type": [
        "Property",
        "EnergyRate"
      ],
      "displayName": {
        "en": "Net Sensible Cooling Capacity"
      },
      "name": "netSensibleCoolingCapacity",
      "schema": "double",
      "unit": "britishThermalUnitPerHour",
      "writable": true,
      "@id": "dtmi:com:syyclops:RefrigerationEvaporatorUnit:netSensibleCoolingCapacity;1",
      "description": {
        "en": "The net sensible cooling capacity property defines the effective cooling output of the refrigeration evaporator unit in thermal units."
      }
    },
    {
      "@type": [
        "Property",
        "ValueAnnotation",
        "Override"
      ],
      "displayName": {
        "en": "Net Sensible Cooling Capacity Unit"
      },
      "name": "netSensibleCoolingCapacityUnit",
      "annotates": "netSensibleCoolingCapacity",
      "overrides": "unit",
      "schema": "PowerUnit",
      "writable": true,
      "@id": "dtmi:com:syyclops:RefrigerationEvaporatorUnit:netSensibleCoolingCapacityUnit;1",
      "description": {
        "en": "The net sensible cooling capacity unit property indicates the measurement unit for the refrigerant's cooling output."
      }
    }
  ],
  "@context": [
    "dtmi:dtdl:context;3",
    "dtmi:dtdl:extension:quantitativeTypes;1",
    "dtmi:dtdl:extension:annotation;1",
    "dtmi:dtdl:extension:overriding;1"
  ]
}