{
  "@id": "dtmi:com:syyclops:RefrigerationCompressor;1",
  "@type": "Interface",
  "displayName": {
    "en": "Refrigeration Compressor"
  },
  "description": {
    "en": "A compressor which is used within a refrigeration system or circuit used in applications such food, beverage, pharmaceutical and chemical storage and ice storage and production."
  },
  "extends": [
    "dtmi:com:syyclops:RefrigerationEquipment;1",
    "dtmi:com:syyclops:Compressor;1"
  ],
  "contents": [
    {
      "@type": "Property",
      "displayName": {
        "en": "Nominal Energy Efficiency Ratio (EER)"
      },
      "name": "nominalEnergyEfficiencyRatio",
      "schema": "double",
      "writable": true,
      "@id": "dtmi:com:syyclops:RefrigerationCompressor:nominalEnergyEfficiencyRatio;1",
      "description": {
        "en": "The nominal energy efficiency ratio indicates the efficiency of the refrigeration compressor under standard operating conditions."
      }
    },
    {
      "@type": "Property",
      "displayName": {
        "en": "Design Energy Efficiency Ratio (EER)"
      },
      "name": "designEnergyEfficiencyRatio",
      "schema": "double",
      "writable": true,
      "@id": "dtmi:com:syyclops:RefrigerationCompressor:designEnergyEfficiencyRatio;1",
      "description": {
        "en": "The design energy efficiency ratio represents the efficiency of the refrigeration compressor as designed."
      }
    },
    {
      "@type": "Property",
      "displayName": {
        "en": "Nominal Coefficient Of Performance (COP)"
      },
      "name": "nominalCoefficientOfPerformance",
      "schema": "double",
      "writable": true,
      "@id": "dtmi:com:syyclops:RefrigerationCompressor:nominalCoefficientOfPerformance;1",
      "description": {
        "en": "The nominal coefficient of performance measures the cooling output of the refrigeration compressor relative to energy input at nominal conditions."
      }
    },
    {
      "@type": "Property",
      "displayName": {
        "en": "Design Coefficient Of Performance (COP)"
      },
      "name": "designCoefficientOfPerformance",
      "schema": "double",
      "writable": true,
      "@id": "dtmi:com:syyclops:RefrigerationCompressor:designCoefficientOfPerformance;1",
      "description": {
        "en": "The design coefficient of performance indicates the theoretical performance efficiency of the refrigeration compressor."
      }
    },
    {
      "@type": [
        "Property",
        "EnergyRate"
      ],
      "displayName": {
        "en": "Nominal Cooling Capacity"
      },
      "name": "nominalCoolingCapacity",
      "schema": "double",
      "unit": "britishThermalUnitPerHour",
      "writable": true,
      "@id": "dtmi:com:syyclops:RefrigerationCompressor:nominalCoolingCapacity;1",
      "description": {
        "en": "The nominal cooling capacity specifies the cooling output of the refrigeration compressor under standard conditions."
      }
    },
    {
      "@type": [
        "Property",
        "ValueAnnotation",
        "Override"
      ],
      "displayName": {
        "en": "Nominal Cooling Capacity Unit"
      },
      "name": "nominalCoolingCapacityUnit",
      "annotates": "nominalCoolingCapacity",
      "overrides": "unit",
      "schema": "PowerUnit",
      "writable": true,
      "@id": "dtmi:com:syyclops:RefrigerationCompressor:nominalCoolingCapacityUnit;1",
      "description": {
        "en": "The nominal cooling capacity unit defines the measurement unit used for the nominal cooling capacity of the compressor."
      }
    },
    {
      "@type": [
        "Property",
        "EnergyRate"
      ],
      "displayName": {
        "en": "Design Cooling Capacity"
      },
      "name": "designCoolingCapacity",
      "schema": "double",
      "unit": "britishThermalUnitPerHour",
      "writable": true,
      "@id": "dtmi:com:syyclops:RefrigerationCompressor:designCoolingCapacity;1",
      "description": {
        "en": "The design cooling capacity property represents the targeted cooling output based on compressor design specifications."
      }
    },
    {
      "@type": [
        "Property",
        "ValueAnnotation",
        "Override"
      ],
      "displayName": {
        "en": "Design Cooling Capacity Unit"
      },
      "name": "designCoolingCapacityUnit",
      "annotates": "designCoolingCapacity",
      "overrides": "unit",
      "schema": "PowerUnit",
      "writable": true,
      "@id": "dtmi:com:syyclops:RefrigerationCompressor:designCoolingCapacityUnit;1",
      "description": {
        "en": "The design cooling capacity unit indicates the measurement unit for the cooling capacity as designed."
      }
    }
  ],
  "@context": [
    "dtmi:dtdl:context;3",
    "dtmi:dtdl:extension:quantitativeTypes;1",
    "dtmi:dtdl:extension:annotation;1",
    "dtmi:dtdl:extension:overriding;1"
  ]
}